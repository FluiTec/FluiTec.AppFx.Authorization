  <ItemGroup>
    <PackageReference Include="FluiTec.AppFx.Data.Dapper.Pgsql" Version="*" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\FluiTec.AppFx.Authorization.Activity.Dapper\FluiTec.AppFx.Authorization.Activity.Dapper.csproj" />
